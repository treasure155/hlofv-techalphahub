/* Gallery Section Styling */
/* Gallery Section Styling */
section {
    background-color: #f9f9f9;
    padding-top: 50px;
    padding-bottom: 50px;
}

/* Gallery Item Styling */
.gallery-item {
    position: relative;
    overflow: hidden;
    border-radius: 10px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
}

/* Hover Effect */
.gallery-item .gallery-img {
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    border-radius: 10px;
    width: 100%;
    height: auto;
}

.gallery-item:hover .gallery-img {
    transform: scale(1.1);
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
}

/* Heading for Gallery */
.heading-primary {
    font-size: 2.5rem;
    font-weight: bold;
    color: #1b479e;
}

</style>
